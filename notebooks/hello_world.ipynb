    "# Hello World Submission
",
    "
",
    "This notebook demonstrates the basic structure for the AI Mathematical Olympiad - Progress Prize 3 submission.
",
    "It uses a mock environment if the `aimo` module is not available (for local testing)."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import sys
",
    "
",
    "# Mock the aimo module for local development
",
    "if 'aimo' not in sys.modules:
",
    "    class MockEnv:
",
    "        def iter_test(self):
",
    "            # Yields (test_df, sample_submission_df) tuples
",
    "            import pandas as pd
",
    "            test_data = pd.DataFrame({'id': ['001', '002'], 'problem': ['1+1=?', '2*2=?']})
",
    "            sample_sub = pd.DataFrame({'id': ['001', '002'], 'answer': [0, 0]})
",
    "            # In reality, this yields one row at a time
",
    "            for i in range(len(test_data)):
",
    "                yield test_data.iloc[[i]], sample_sub.iloc[[i]]
",
    "        
",
    "        def predict(self, submission):
",
    "            print(f"Predicted: {submission['answer'].values[0]}")
",
    "
",
    "    class AIMO:
",
    "        def make_env(self):
",
    "            return MockEnv()
",
    "    
",
    "    # Inject mock
",
    "    import types
",
    "    aimo = types.ModuleType('aimo')
",
    "    aimo.make_env = AIMO().make_env
",
    "    sys.modules['aimo'] = aimo
",
    "    print("Mock 'aimo' module loaded.")
",
    "else:
",
    "    import aimo
",
    "    print("Real 'aimo' module loaded.")"
   ]
  },
